{
  "manifest_version": 2,
  "name": "BNB Browser",
  "description": "Online wallet for Binance Chain",
  "version": "0.0.1",
  "browser_action": {
    "default_icon": {
      "16": "assets/icons/icon16.png",
      "48": "assets/icons/icon48.png",
      "128": "assets/icons/icon128.png"
    },
    "default_popup": "index.html"
  },
  "background": {
    "scripts": [
      "polyfills.bebee6a5ef0ece001bc6.js",
      "runtime.26209474bfa8dc87a77c.js",
      "scripts.1dfa26ab637d7ce00e45.js",
      "main.8899ca4fab7c58489528.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
